{
    "name": "RgbToFloatMap",
    "type": "Map",
    "attributes": {
        "attrInput": {
            "name": "input",
            "type": "Rgb",
            "default": "Rgb(1.0f, 1.0f, 1.0f)",
            "flags": "FLAGS_BINDABLE",
            "comment": "Input color to convert"
        },
        "attrMode" : {
            "name" : "mode",
            "type" : "Int",
            "flags": "FLAGS_ENUMERABLE",
            "default": "5",
            "enum": {
                "r" : "0",
                "g" : "1",
                "b" : "2",
                "min" : "3",
                "max" : "4",
                "average" : "5",
                "sum" : "6",
                "luminance" : "7"
            },
            "comment" : "The method to convert RGB Color to float"
        }
    }
}
